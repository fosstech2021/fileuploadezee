<!-- Call To Section -->
<section class="call-to-section">
    <div class="auto-container">
        <div class="inner clearfix">
            <div class="shape-1 wow slideInRight" data-wow-delay="0ms" data-wow-duration="1500ms"></div>
            <div class="shape-2 wow fadeInDown" data-wow-delay="0ms" data-wow-duration="1500ms"></div>
            <h2>Let's Get  <br>Started!</h2>
            <div class="link-box">
                <a class="theme-btn btn-style-two" asp-action="Contact" asp-controller="Home">
                    <i class="btn-curve"></i>
                    <span class="btn-title">Contact with us</span>
                </a>
            </div>
        </div>
    </div>
</section>

<!-- Main Footer -->
<footer class="main-footer">
    <div class="auto-container">
        <!--Widgets Section-->
        <div class="widgets-section">
            <div class="row clearfix">

                <!--Column-->
                <div class="column col-xl-3 col-lg-6 col-md-6 col-sm-12">
                    <div class="footer-widget logo-widget">
                        <div class="widget-content">
                            <div class="logo">
                                <a asp-action="Index" asp-controller="Home"><img id="fLogo" src="@Model.Logo.FooterLogo" alt="" /></a>
                            </div>
                            <div class="text">
                                Welcome to EZEE AGREEMENT - COMPLETE HOME AGREEMENT SOLUTION

                            </div>
                            <ul class="social-links clearfix">
                                <li><a href="@Model.BusinessProfile.FacebookPageURL"><span class="fab fa-facebook-square"></span></a></li>
                                <li><a href="@Model.BusinessProfile.TwitterURL"><span class="fab fa-twitter"></span></a></li>
                                <li><a href="@Model.BusinessProfile.InstagramPageURL"><span class="fab fa-instagram"></span></a></li>
                               
                            </ul>
                        </div>
                    </div>
                </div>

                <!--Column-->
                <div class="column col-xl-3 col-lg-6 col-md-6 col-sm-12">
                    <div class="footer-widget links-widget">
                        <div class="widget-content">
                            <h6>Explore</h6>
                            <div class="row clearfix">
                                <div class="col-md-6 col-sm-12">
                                    <ul>
                                        @foreach (var menu in Model.Menus)
                                        {
                                            @if (menu.Type == "internal" || menu.Type == null)
                                            {
                                                <li><a href="@menu.Url">@menu.Name</a></li>
                                            }

                                        }
                                        
                                    </ul>
                                </div>
                               
                            </div>
                        </div>
                    </div>
                </div>

                <!--Column-->
                <div class="column col-xl-3 col-lg-6 col-md-6 col-sm-12">
                    <div class="footer-widget info-widget">
                        <div class="widget-content">
                            <h6>Contact</h6>
                            <ul class="contact-info">
                                <li class="address">
                                    <span class="icon flaticon-pin-1"></span>@Model.BusinessProfile.Address <br>@Model.BusinessProfile.Country
                                </li>
                                <li>
                                    <span class="icon flaticon-call"></span><a href="tel:@Model.BusinessProfile.DisplayContact1">
                                        @Model.BusinessProfile.DisplayContact1
                                    </a>
                                </li>
                                <li>
                                    <span class="icon flaticon-call"></span><a href="tel:@Model.BusinessProfile.DisplayContact2">
                                        @Model.BusinessProfile.DisplayContact2
                                    </a>
                                </li>
                                <li>
                                    <span class="icon flaticon-email-2"></span><a href="mailto:@Model.BusinessProfile.EmailAddress">@Model.BusinessProfile.EmailAddress</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>

                <!--Column-->
                <div class="column col-xl-3 col-lg-6 col-md-6 col-sm-12">
                    <div class="footer-widget newsletter-widget">
                        <div class="widget-content">
                            <h6>Newsletter</h6>
                            <div class="newsletter-form">
                                <form method="post" action="#">
                                    <div class="form-group clearfix">
                                        <input type="email" name="email" value="" placeholder="Email Address"
                                               required="">
                                        <button type="submit" class="theme-btn">
                                            <span class="fa fa-envelope"></span>
                                        </button>
                                    </div>
                                </form>
                            </div>
                            <div class="text">
                                Sign up for our latest news & articles. We won’t give you spam
                                mails.
                            </div>
                        </div>
                    </div>
                </div>

            </div>

        </div>
    </div>

    <!-- Footer Bottom -->
    <div class="footer-bottom">
        <div class="auto-container">
            <div class="inner clearfix">
                <div class="copyright">&copy; copyright @Model.BusinessProfile.Copyright</div>
            </div>
        </div>
    </div>

</footer>