@model IEnumerable<BasePackageModule1.Models.Facility>

@{
    ViewData["Title"] = "Index";
}

<section class="breadcrumb-area d-flex align-items-center" style="background-image:url(theme/img/testimonial/test-bg.jpg)">
    <div class="container">
        <div class="row">
            <div class="col-xl-6 offset-xl-3 col-lg-8 offset-lg-2">
                <div class="breadcrumb-wrap text-center">
                    <div class="breadcrumb-title mb-30">
                        <h2>facilities</h2>
                    </div>
                    <nav aria-label="breadcrumb">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a asp-action="Index" asp-controller="Home">Home</a></li>

                        </ol>
                    </nav>
                </div>
            </div>
        </div>
    </div>
</section>

<section class="department-area cta-bg pb-70 pt-100 fix" style="background-image:url(theme/img/an-bg/an-bg05.png); background-size: contain;">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-12">
                <ul>
                    @foreach (var doctor in Model)
                    {
                        <li>

                            <a href="#" class="text">
                                <h3>@doctor.Name</h3>
                                @doctor.Name
                            </a>
                        </li>
                    }


                </ul>

            </div>

        </div>
    </div>
</section>